<core:View
    xmlns:core="sap.ui.core"
    xmlns:l="sap.ui.layout"
    xmlns="sap.m"
    height="100%"
    xmlns:tnt="sap.tnt"
    xmlns:sing="MyUI5WebApp.controls.signature"
    xmlns:custom="MyUI5WebApp.controls"
    xmlns:quillEditor="MyUI5WebApp.controls.quill"
    xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
    controllerName="MyUI5WebApp.src.pages.detalhes.detalhes">

    <Page title="{i18n>Commom.Detalhes}">
        <content showNavButton="true" navButtonPress=".onNavBack> ">
            <ObjectHeader
                title="ID: {todo>Id}  "
            >

                <ObjectAttribute text="UserID: {todo>userId}" />
                <ObjectAttribute text="Title: {todo>title}" />
                <ObjectAttribute text="Completed:  {todo>completed}" />
            </ObjectHeader>
        </content>
    </Page>
</core:View>