{
  "_version": "1.0.1",
  "sap.app": {
    "_version": "1.0.0",
    "id": "MyUI5WebApp",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "MyUI5WebApp",
    "description": "Descrição do aplicativo",
    "applicationVersion": {
      "version": "1.0.0"
    }
  },
  "sap.ui": {
    "_version": "1.3.0",
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "supportedThemes": [
      { "Id": "sap_belize", "Description": "Belize" },
      { "Id": "sap_belize_plus", "Description": "Belize Plus" },
      { "Id": "sap_bluecrystal", "Description": "Blue Crystal" },
      { "Id": "sap_belize_hcb", "Description": "Alto Constraste Preto" },
      { "Id": "sap_belize_hcw", "Description": "Alto Contraste Branco" }
    ]
  },
  "sap.ui5": {
    "_version": "1.2.0",
    "rootView": "MyUI5WebApp.src.app.App",
    "config": {
      "serviceUrl": "https://jsonplaceholder.typicode.com",
      "localDataUrl": "webapp/model/data"
    },
    "dependencies": {
      "minUI5Version": "1.34.0",
      "libs": {
        "sap.ui.core": {
          "minVersion": "1.34.0"
        },
        "sap.m": {
          "minVersion": "1.34.0"
        },
        "sap.ui.layout": {
          "minVersion": "1.34.0"
        }
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "MyUI5WebApp.i18n.i18n"
        }
      },
      "todo": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "https://jsonplaceholder.typicode.com/todos"
      }
    },

    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "controlId": "app",
        "controlAggregation": "pages",
        "bypassed": {
          "target": "notFound"
        }
      },

      "routes": [
        {
          "pattern": "app",
          "name": "app",
          "target": "app"
        },
        {
          "pattern": "",
          "name": "login",
          "target": "login"
        },
        {
          "pattern": "listaTarefas",
          "name": "listaTarefas",
          "target": "listaTarefas"
        },
        {
          "pattern": "settings/:userName:",
          "name": "settings",
          "target": "settings"
        },
        {
          "pattern": "exemplos",
          "name": "exemplos",
          "target": "exemplos"
        },
        {
          "pattern": "languages",
          "name": "languages",
          "target": "languages"
        }
      ],

      "targets": {
        "listaTarefas": {
          "viewName": "ListaTarefas",
          "viewLevel": 2,
          "transition": "flip",
          "viewPath": "MyUI5WebApp.src.pages.listaTarefas"
        },
        "settings": {
          "viewName": "Edit",
          "viewLevel": 3,
          "viewPath": "MyUI5WebApp.src.pages.settings",
          "transition": "flip"
        },
        "login": {
          "viewName": "Login",
          "viewLevel": 3,
          "viewPath": "MyUI5WebApp.src.pages.security",
          "transition": "flip"
        },
        "exemplos": {
          "viewName": "CustomControls",
          "viewLevel": 3,
          "viewPath": "MyUI5WebApp.src.pages.exemplos",
          "transition": "flip"
        },
        "app": {
          "viewName": "App",
          "viewLevel": 3,
          "viewPath": "MyUI5WebApp.src.app"
        },
        "languages": {
          "viewName": "Languages",
          "viewLevel": 3,
          "viewPath": "MyUI5WebApp.src.pages.languages"
        },
        "detalhes": {
          "viewName": "detalhes",
          "viewLevel": 3,
          "viewPath": "MyUI5WebApp.src.pages.detalhes"
        }
      }
    }
  },

  "name": "MyUI5WebApp",
  "short_name": "Invent",
  "start_url": "index.html",
  "display": "standalone",
  "orientation": "portrait",
  "icons": [
    {
      "src": "img/icons/icon-72x72.png",
      "sizes": "72x72",
      "type": "image/png"
    },
    {
      "src": "img/icons/icon-96x96.png",
      "sizes": "96x96",
      "type": "image/png"
    },
    {
      "src": "img/icons/icon-128x128.png",
      "sizes": "128x128",
      "type": "image/png"
    },
    {
      "src": "img/icons/icon-144x144.png",
      "sizes": "144x144",
      "type": "image/png"
    },
    {
      "src": "img/icons/icon-152x152.png",
      "sizes": "152x152",
      "type": "image/png"
    },
    {
      "src": "img/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "img/icons/icon-384x384.png",
      "sizes": "384x384",
      "type": "image/png"
    },
    {
      "src": "img/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "background_color": "#3F5161",
  "theme_color": "#3F5161"
}
